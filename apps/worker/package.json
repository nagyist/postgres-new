{
  "name": "@postgres-new/worker",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "start:node": "node --env-file=.env --experimental-strip-types src/index.ts",
    "start:deno": "deno run --config deno.json --env --allow-all src/index.ts",
    "start:bun": "bun src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@electric-sql/pglite": "0.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "tsup": "^8.2.4",
    "typescript": "^5.5.3"
  }
}
